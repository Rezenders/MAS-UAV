[teste]
comm_method = topic
comm_name = /hw/teste
comm_msg = String
dependencies = std_msgs.msg

[set_mode]
comm_method = service
comm_name = /mavros/set_mode
comm_msg = SetMode
dependencies = mavros_msgs.srv
params_name = custom_mode

[arm_motors]
comm_method = service
comm_name = mavros/cmd/arming
comm_msg = CommandBool
dependencies = mavros_msgs.srv
params_name = value
params_type = bool

[takeoff]
