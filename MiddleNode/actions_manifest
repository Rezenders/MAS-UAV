comm_name = /mavros/cmd/takeoff
comm_msg = CommandTOL
dependencies = mavros_msgs.srv
params_name = altitude
params_type = float

[setpoint]
comm_method = topic
comm_name = /mavros/setpoint_position/global
comm_msg = GlobalPositionTarget
dependencies = mavros_msgs.msg
params_name = latitude, longitude, altitude
params_type = float, float, float

[land]
